{
    "problemMatcher": [
        {
            "owner": "shellcheck-gcc",
            "pattern": [
                {
                    "regexp": "^(.*?)\\s*:\\s*(\\d+)\\s*:\\s*(\\d+)\\s*:\\s*(error|warning|note)\\s*:\\s*(.*)\\s*\\[(.*?)\\]$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }
    ]
}
